{
    "name": "universetech-inc/watchdog",
    "type": "library",
    "description": "The watchdog package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "watchdog",
        "hypervel"
    ],
    "autoload": {
        "psr-4": {
            "UniverseTech\\Watchdog\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "hypervel/foundation": "^0.1 || ^0.2 || ^0.3",
        "hypervel/coroutine": "^0.1 || ^0.2 || ^0.3",
        "hypervel/support": "^0.1 || ^0.2 || ^0.3",
        "symfony/console": "^5.4 || ^6.4 || ^7.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "UniverseTech\\Watchdog\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    }
}
